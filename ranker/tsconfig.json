{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node 14",
  
    "compilerOptions": {
      "experimentalDecorators": true,
      "lib": ["es2020"],
      "module": "commonjs",
      "target": "es2020",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "dist",
      "sourceMap": true
    }  
  }